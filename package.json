{
  "name": "sfc",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start:inputs": "ng serve sfc-inputs-app",
    "start:components": "ng serve sfc-components-app",
    "build": "ng build",
    "build:common": "ng build ngx-sfc-common && npm run build:scss",
    "build:inputs": "ng build ngx-sfc-inputs",
    "build:components": "ng build ngx-sfc-components",
    "build:all": "npm run build:common && npm run build:inputs && npm run build:components",
    "build:scss": "scss-bundle -c scss-bundle.config.json",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "test:common": "ng test ngx-sfc-common",
    "test:inputs": "ng test ngx-sfc-inputs",
    "test:components": "ng test ngx-sfc-components",
    "test:all": "npm run test:common && npm run test:inputs && npm run test:components",
    "test:no-browser:all": "ng test ngx-sfc-common --watch=false --browsers=ChromeHeadless && ng test ngx-sfc-inputs --watch=false --browsers=ChromeHeadless && ng test ngx-sfc-components --watch=false --browsers=ChromeHeadless",
    "test:code-coverage:common": "ng test ngx-sfc-common --no-watch --code-coverage",
    "test:code-coverage:inputs": "ng test ngx-sfc-inputs --no-watch --code-coverage",
    "test:code-coverage:components": "ng test ngx-sfc-components --no-watch --code-coverage",
    "test:code-coverage:all": "npm run test:code-coverage:common && npm run test:code-coverage:inputs && npm run test:code-coverage:components",
    "lint": "ng lint",
    "lint:es:common": "ng lint ngx-sfc-common",
    "lint:es:inputs": "ng lint ngx-sfc-inputs",
    "lint:es:components": "ng lint ngx-sfc-components",
    "lint:es:all": "npm run lint:es:common && npm run lint:es:inputs && npm run lint:es:components",
    "lint:styles": "npx stylelint \"**/*.scss\" --fix",
    "lint:all": "npm run lint:es:all && npm run lint:styles"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^14.2.12",
    "@angular/common": "^14.2.12",
    "@angular/compiler": "^14.2.12",
    "@angular/core": "^14.2.12",
    "@angular/forms": "^14.2.12",
    "@angular/platform-browser": "^14.2.12",
    "@angular/platform-browser-dynamic": "^14.2.12",
    "@angular/router": "^14.2.12",
    "chart.js": "^3.6.0",
    "cropperjs": ">=1.5.7",
    "@fortawesome/angular-fontawesome": "0.10.2",
    "@fortawesome/fontawesome-svg-core": "^6.1.1",
    "@fortawesome/free-solid-svg-icons": "^6.1.1",
    "@fortawesome/free-regular-svg-icons": "^6.1.1",
    "ngx-sfc-common": "^0.0.5",
    "ngx-sfc-components": "^0.0.3",
    "rxjs": "~7.5.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^14.2.10",
    "@angular-eslint/builder": "13.1.0",
    "@angular-eslint/eslint-plugin": "13.1.0",
    "@angular-eslint/eslint-plugin-template": "13.1.0",
    "@angular-eslint/schematics": "13.1.0",
    "@angular-eslint/template-parser": "13.1.0",
    "@angular/cli": "^14.2.10",
    "@angular/compiler-cli": "^14.2.12",
    "@types/jasmine": "~3.10.0",
    "@types/node": "^12.11.1",
    "@typescript-eslint/eslint-plugin": "5.11.0",
    "@typescript-eslint/parser": "5.11.0",
    "eslint": "^8.2.0",
    "husky": "^4.3.8",
    "jasmine-core": "~4.0.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.1.0",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "~1.7.0",
    "ng-packagr": "^14.2.2",
    "scss-bundle": "3.1.2",
    "stylelint": "^14.6.0",
    "stylelint-config-standard-scss": "^3.0.0",
    "typescript": "~4.8.4"
  }
}