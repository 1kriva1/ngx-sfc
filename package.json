{
  "name": "sfc",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start:inputs": "ng serve sfc-inputs-app",
    "start:components": "ng serve sfc-components-app",
    "build:common": "ng build ngx-sfc-common",
    "build:common:watch": "ng build ngx-sfc-common --watch",
    "build:inputs": "ng build ngx-sfc-inputs",
    "build:inputs:watch": "ng build ngx-sfc-inputs --watch",
    "build:components": "ng build ngx-sfc-components",
    "build:components:watch": "ng build ngx-sfc-components --watch",
    "build:all": "npm run build:common && npm run build:inputs && npm run build:components",
    "test:common": "ng test ngx-sfc-common",
    "test:inputs": "ng test ngx-sfc-inputs",
    "test:components": "ng test ngx-sfc-components",
    "test:all": "npm run test:common && npm run test:inputs && npm run test:components",
    "test:no-browser:all": "ng test ngx-sfc-common --watch=false --browsers=ChromeHeadless && ng test ngx-sfc-inputs --watch=false --browsers=ChromeHeadless && ng test ngx-sfc-components --watch=false --browsers=ChromeHeadless",
    "test:code-coverage:common": "ng test ngx-sfc-common --no-watch --code-coverage",
    "test:code-coverage:inputs": "ng test ngx-sfc-inputs --no-watch --code-coverage",
    "test:code-coverage:components": "ng test ngx-sfc-components --no-watch --code-coverage",
    "test:code-coverage:all": "npm run test:code-coverage:common && npm run test:code-coverage:inputs && npm run test:code-coverage:components",
    "lint": "ng lint",
    "lint:es:common": "ng lint ngx-sfc-common",
    "lint:es:inputs": "ng lint ngx-sfc-inputs",
    "lint:es:components": "ng lint ngx-sfc-components",
    "lint:es:all": "npm run lint:es:common && npm run lint:es:inputs && npm run lint:es:components",
    "lint:styles": "npx stylelint \"**/*.scss\" --fix",
    "lint:all": "npm run lint:es:all && npm run lint:styles"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^15.0.4",
    "@angular/common": "^15.0.4",
    "@angular/compiler": "^15.0.4",
    "@angular/core": "^15.0.4",
    "@angular/forms": "^15.0.4",
    "@angular/platform-browser": "^15.0.4",
    "@angular/platform-browser-dynamic": "^15.0.4",
    "@angular/router": "^15.0.4",
    "chart.js": "^3.6.0",
    "cropperjs": ">=1.5.7",
    "@fortawesome/angular-fontawesome": "0.10.2",
    "@fortawesome/fontawesome-svg-core": "^6.1.1",
    "@fortawesome/free-solid-svg-icons": "^6.1.1",
    "@fortawesome/free-regular-svg-icons": "^6.1.1",
    "ngx-sfc-common": "^0.0.14",
    "ngx-sfc-components": "^0.0.12",
    "ngx-sfc-inputs": "^0.0.8",
    "rxjs": "~7.5.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^15.0.4",
    "@angular-eslint/builder": "^15.1.0",
    "@angular-eslint/eslint-plugin": "^15.1.0",
    "@angular-eslint/eslint-plugin-template": "^15.1.0",
    "@angular-eslint/schematics": "^15.1.0",
    "@angular-eslint/template-parser": "^15.1.0",
    "@angular/cli": "^15.0.4",
    "@angular/compiler-cli": "^15.0.4",
    "@types/jasmine": "~3.10.0",
    "@types/node": "^12.11.1",
    "@typescript-eslint/eslint-plugin": "^5.47.1",
    "@typescript-eslint/parser": "^5.47.1",
    "eslint": "^8.2.0",
    "husky": "^4.3.8",
    "jasmine-core": "~4.0.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.1.0",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "~1.7.0",
    "ng-packagr": "^15.0.3",
    "scss-bundle": "3.1.2",
    "stylelint": "^14.6.0",
    "stylelint-config-standard-scss": "^3.0.0",
    "typescript": "~4.8.4",
    "concurrently": "^7.6.0"
  }
}